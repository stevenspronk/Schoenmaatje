<mvc:View controllerName="Schoenmaatje.controller.Overview" displayBlock="true" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>homePageTitle}">
				<headerContent></headerContent>
				<content>
					<VBox>
					<Button id="btn_scanSender" press="scanBarcode" text="{i18n>Scan Sender}"></Button>
					<Input id="btn_goToSender" submit="goToSender" text="{i18n>Sender}"/>
					<Button id="btn_goToReceiver" press="goToReceiver" text="{i18n>Receiver}"/>
					<Button id="btn_goToTimeline" press="goToTimeline" text="{i18n>Timeline}"/>
					<Button id="btn_goToCountry" press="goToCountry" text="{i18n>Country}"/>
					<Text text="Lorem ipsum"/>
					<Button press="scanBarcode" text="Scan!"/>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>